{
  "name": "rss",
  "version": "0.0.1",
  "private": true,
  "description": "rss",
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "prebuild": "npm run build:renderer",
    "preelectron-dev": "npm run build:renderer",
    "preelectron-dist": "npm run build",
    "electron-dev": "cross-env BROWSER=none concurrently \"npm run start\" \"wait-on http://localhost:3000 && npm run electron-start\"",
    "electron-dist": "electron-builder build",
    "electron-pack": "electron-builder build",
    "electron-start": "electron .",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:renderer": "webpack --config ./main/webpack.config.js",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint --ext .ts,.tsx src main",
    "test": "react-app-rewired test",
    "test:coverage": "react-app-rewired test --coverage",
    "eject": "react-scripts eject",
    "release": "standard-version",
    "analyze": "npm run build -- --analyze",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "dependencies": {
    "chardet": "^0.8.0",
    "custom-electron-titlebar": "^3.1.0",
    "electron-is-dev": "^1.0.1",
    "electron-updater": "^4.1.2",
    "feedparser": "^2.2.9",
    "hox": "^1.0.0",
    "iconv-lite": "^0.5.0"
  },
  "devDependencies": {
    "@jest-runner/electron": "^2.0.2",
    "@types/chardet": "^0.8.0",
    "@types/feedparser": "^2.2.3",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.8",
    "@types/pouchdb-adapter-memory": "^6.1.3",
    "@types/pouchdb-browser": "^6.1.3",
    "@types/pouchdb-find": "^6.3.4",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "babel-plugin-import": "^1.12.2",
    "concurrently": "^5.0.0",
    "coveralls": "^3.0.5",
    "cross-env": "^6.0.0",
    "customize-cra": "^0.9.1",
    "electron": "^6.0.2",
    "electron-builder": "^21.1.5",
    "eslint": "^6.6.0",
    "husky": "^3.0.3",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.4.2",
    "pouchdb-adapter-memory": "^7.1.1",
    "pouchdb-browser": "^7.1.1",
    "pouchdb-find": "^7.1.1",
    "prettier": "^1.19.1",
    "react": "^16.10.1",
    "react-app-rewired": "^2.1.0",
    "react-dom": "^16.10.1",
    "react-scripts": "3.2.0",
    "standard-version": "^7.0.0",
    "svg-sprite-loader": "^4.1.6",
    "ts-loader": "^6.0.4",
    "typescript": "3.7.2",
    "wait-on": "^3.3.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.6"
  },
  "lint-staged": {
    "*.{ts,tsx,jsx,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
